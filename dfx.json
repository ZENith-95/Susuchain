{
  "version": 1,
  "canisters": {
    "susuchain": {
      "type": "motoko",
      "main": "src/susuchain-app-backend/susuchain.mo",
      "declarations": {
        "output": "src/declarations/susuchain",
        "node_compatibility": true
      }
    },
    "group-contributions": {
      "type": "motoko",
      "main": "src/susuchain-app-backend/susu-group-contributions.mo",
      "declarations": {
        "output": "src/declarations/group-contributions",
        "node_compatibility": true
      }
    },
    "token": {
      "type": "custom",
      "candid": "https://raw.githubusercontent.com/dfinity/ic/2e3589427cd9648d4edaebc1b96b5daf8fdd94d8/rs/rosetta-api/icrc1/ledger/ledger.did",
      "wasm": "https://download.dfinity.systems/ic/2e3589427cd9648d4edaebc1b96b5daf8fdd94d8/canisters/ic-icrc1-ledger.wasm.gz",
      "declarations": {
        "output": "src/declarations/token",
        "node_compatibility": true
      }
    },
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
      "id": "rdmx6-jaaaa-aaaaa-aaadq-cai"
    },
    "frontend": {
      "type": "assets",
      "source": [
        "src/susuchain-frontend/out"
      ],
      "build": [
        "cd src/susuchain-frontend && npm run build"
      ],
      "declarations": {
        "output": "src/declarations/frontend",
        "node_compatibility": true
      }
    }
  },
  "output_env_file": ".env",
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  }
}